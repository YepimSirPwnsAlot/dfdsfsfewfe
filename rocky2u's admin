-- StarterGui.c00lguiFE.Frame.Page3.LocalPlayer.Floating Pad.LocalScript
local function C_c2()
	local script = G2L["c2"];
	function click()
		local name = game.Players.LocalPlayer.Name
		local TweenService = game:GetService("TweenService")

		local p = Instance.new("Part")
		p.Parent = workspace
		p.Locked = true
		p.BrickColor = BrickColor.new("White")
		p.BrickColor = BrickColor.new(104)
		p.Size = Vector3.new(8, 1.2, 8)
		p.Anchored = true
		local m = Instance.new("CylinderMesh")
		m.Scale = Vector3.new(1, 0.5, 1)
		m.Parent = p
		while true do
			local targetPlayer = game.Players:FindFirstChild(name)
			if targetPlayer then
				local targetTorso = targetPlayer.Character and targetPlayer.Character:FindFirstChild("Torso")
				if targetTorso then
					local targetCFrame = targetTorso.CFrame * CFrame.new(0, -4, 0)

					local tweenInfo = TweenInfo.new(0.1, Enum.EasingStyle.Linear) -- Customize the easing style

					local tween = TweenService:Create(p, tweenInfo, {CFrame = targetCFrame})
					tween:Play()
				end
			end
			wait(0.1)
		end
	end

	script.Parent.MouseButton1Down:connect(click)
end;
task.spawn(C_c2);
